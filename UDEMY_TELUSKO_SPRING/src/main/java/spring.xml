<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

 <!-- bean means class or object, property means it variables or reference variables -->
	<bean id="alien" class="com.shah.Spring.Alien" scope="prototype">
		<property name="age" value="12"></property>
		<property name="laptop" ref="laptop"></property>
	</bean>
	
	<bean id="laptop" class="com.shah.Spring.Laptop"></bean>
</beans>
